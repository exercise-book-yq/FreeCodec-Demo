		</div>
	</nav>

    <!-- Page Header -->
    <header class="masthead" style="background-image: url('img/home-bg.jpg')">
        <div class="container" style="padding-bottom:10px">
            <div class="row">
                <div class="col-lg-10 col-md-10 mx-auto">
                    <div class="site-heading">
                        <h1>FreeCodec Demo</h1>
                        <span class="subheading">
                            FreeCodec
                        </span>
                    </div>
                </div>
            </div>
			
            
			
        </div>
    </header>

    <!-- Main Content -->
    <div class="container">
        <div class="row">
            <div class="col-lg-14 col-md-12 mx-auto">
                <div class="post-preview" id="abstract">
                    <h2 class="post-title">
                        Abstract
                    </h2>
                    <p>
                        
					</p>
                </div>
				
							
				
                <div class="post-preview" id="speech coding">
                    <h2 class="post-title">
                        Speech coding
                    </h2>
                    <h3 class="post-meta" id="vctk">
                        Unseen Speakers-VCTK 
                    </h3>
                    <table class="table">
                        <thead>
                            <tr>
                                <th scope="col">Target</th>
                                <th scope="col">Ours</th>
								<th scope="col">Retrain Models</th>
								<th scope="col">Others</th>							
                            </tr>
                        </thead>
                        <tbody>
                            {% for basename,  tgt, dacodec_1, dacodec_2, dacodec_3, encodec_1, encodec_2, encodec_3,  encodec_5, lyra_1, lyra_2, opus_1, opus_2, speex in s2s_rows%}
                            <tr>
                                <td>{{ basename }}
                                    <audio controls preload="none">
                                        <source src="{{ tgt }}">
                                    </audio>
                                </td>
                                <td>
									Ours-0.45kbps
                                    <audio controls preload="none">
                                        <source src="{{ dacodec_1 }}">
                                    </audio> 
									Ours-nodistill
                                    <audio controls preload="none">
                                        <source src="{{ dacodec_2 }}">
                                    </audio>
									Ours-spk_vq
                                    <audio controls preload="none">
                                        <source src="{{ dacodec_3 }}">
                                    </audio>
                                </td>
								<td>
									TiCodec-500bps(retrained)
                                    <audio controls preload="none">
                                        <source src="{{ encodec_1 }}">
                                    </audio> 
									TiCodec-1000bps(retrained)
                                    <audio controls preload="none">
                                        <source src="{{ encodec_2 }}">
                                    </audio>
									DAC-500bps(retrained)
                                    <audio controls preload="none">
                                        <source src="{{ encodec_3 }}">
                                    </audio>
									DAC-1000bps(retrained)
                                    <audio controls preload="none">
                                        <source src="{{ encodec_5 }}">
                                    </audio> 
                                </td>
            
								<td>
									Lyra-v1-3kbps
                                    <audio controls preload="none">
                                        <source src="{{ lyra_1 }}">
                                    </audio> 
									Lyra-v2-3.2kbps
                                    <audio controls preload="none">
                                        <source src="{{ lyra_2 }}">
                                    </audio>
									Opur-6kbps
                                    <audio controls preload="none">
                                        <source src="{{ opus_1 }}">
                                    </audio> 
									Opus-9kbps
                                    <audio controls preload="none">
                                        <source src="{{ opus_2 }}">
                                    </audio>
								    Speex-4kbps
                                    <audio controls preload="none">
                                        <source src="{{ speex }}">
                                    </audio> 
                                </td>
                            </tr>
                            {% endfor %}
                        </tbody>
                    </table>

                    <h3 class="post-meta" id="vctk">
                        Unseen Speakers-LibriSpeech-Test-clean 
                    </h3>
                    <table class="table">
                        <thead>
                            <tr>
                                <th scope="col">Target</th>
                                <th scope="col">Ours</th>
								<th scope="col">Retrain Models</th>
								<th scope="col">Others</th>							
                            </tr>
                        </thead>
                        <tbody>
                            {% for basename,  tgt, dacodec_1, dacodec_2, dacodec_3, encodec_1, encodec_2, encodec_3,  encodec_5, lyra_1, lyra_2, opus_1, opus_2, speex in u2s_rows%}
                            <tr>
                                <td>{{ basename }}
                                    <audio controls preload="none">
                                        <source src="{{ tgt }}">
                                    </audio>
                                </td>
                                <td>
									Ours-0.45kbps
                                    <audio controls preload="none">
                                        <source src="{{ dacodec_1 }}">
                                    </audio> 
									Ours-nodistill
                                    <audio controls preload="none">
                                        <source src="{{ dacodec_2 }}">
                                    </audio>
									Ours-spk_vq
                                    <audio controls preload="none">
                                        <source src="{{ dacodec_3 }}">
                                    </audio>
                                </td>
								<td>
									TiCodec-500bps(retrained)
                                    <audio controls preload="none">
                                        <source src="{{ encodec_1 }}">
                                    </audio> 
									TiCodec-1000bps(retrained)
                                    <audio controls preload="none">
                                        <source src="{{ encodec_2 }}">
                                    </audio>
									DAC-500bps(retrained)
                                    <audio controls preload="none">
                                        <source src="{{ encodec_3 }}">
                                    </audio>
									DAC-1000bps(retrained)
                                    <audio controls preload="none">
                                        <source src="{{ encodec_5 }}">
                                    </audio> 
                                </td>
								<td>
									Lyra-v1-3kbps
                                    <audio controls preload="none">
                                        <source src="{{ lyra_1 }}">
                                    </audio> 
									Lyra-v2-3.2kbps
                                    <audio controls preload="none">
                                        <source src="{{ lyra_2 }}">
                                    </audio>
									Opur-6kbps
                                    <audio controls preload="none">
                                        <source src="{{ opus_1 }}">
                                    </audio> 
									Opus-9kbps
                                    <audio controls preload="none">
                                        <source src="{{ opus_2 }}">
                                    </audio>
								    Speex-4kbps
                                    <audio controls preload="none">
                                        <source src="{{ speex }}">
                                    </audio> 
                                </td>
                            </tr>
                            {% endfor %}
                        </tbody>
                    </table>
                </div>
                <div class="post-preview" id="voice conversion">
                    <h2 class="post-title">
                        Voice Conversion
                    </h2>
                <h3 class="post-meta" id="vc_s2s">
                        Test-clean to VCTK
                    </h3>
                    <table class="table">
                        <thead>
                            <tr>
                                <th scope="col">Source</th>
                                <th scope="col">Target</th>
                                <th scope="col">Conversion</th>
                            </tr>
                        </thead>
                        <tbody>
                            {% for src_basename, tgt_basename, src, tgt, ours, yourtts, wav2vec_vc in vc_s2s_rows%}
                            <tr>
                                <td>{{ src_basename }}
                                    <audio controls preload="none">
                                        <source src="{{ src }}">
                                    </audio>
                                </td>
                                <td>{{ tgt_basename }}
                                    <audio controls preload="none">
                                        <source src="{{ tgt }}">
                                    </audio>
                                </td>
                                <td>
									Wav2vec-VC
                                    <audio controls preload="none">
                                        <source src="{{ wav2vec_vc }}">
                                    </audio> 
									YourTTS
                                    <audio controls preload="none">
                                        <source src="{{ yourtts }}">
                                    </audio>
                                </td>
                                <td>
								    Ours
                                    <audio controls preload="none">
                                        <source src="{{ ours }}">
                                    </audio> 
                                </td>
                            </tr>
                            {% endfor %}
                        </tbody>
                    </table>
                    <h3 class="post-meta" id="vc_u2s">
                        Test-clean to Test-clean
                    </h3>
                    <table class="table">
                        <thead>
                            <tr>
                                <th scope="col">Source</th>
                                <th scope="col">Target</th>
                                <th scope="col">Conversion</th>
                            </tr>
                        </thead>
                        <tbody>
                            {% for src_basename, tgt_basename, src, tgt, ours, yourtts, wav2vec_vc in vc_u2s_rows%}
                            <tr>
                                <td>{{ src_basename }}
                                    <audio controls preload="none">
                                        <source src="{{ src }}">
                                    </audio>
                                </td>
                                <td>{{ tgt_basename }}
                                    <audio controls preload="none">
                                        <source src="{{ tgt }}">
                                    </audio>
                                </td>
                                <td>
									Wav2vec-VC
                                    <audio controls preload="none">
                                        <source src="{{ wav2vec_vc }}">
                                    </audio> 
									YourTTS
                                    <audio controls preload="none">
                                        <source src="{{ yourtts }}">
                                    </audio>
                                </td>
                                <td>
								    Ours
                                    <audio controls preload="none">
                                        <source src="{{ ours }}">
                                    </audio> 
                                </td>
                            </tr>
                            {% endfor %}
                        </tbody>
                    </table>
                </div>
            
                
				
            </div>
        </div>
		</div>


</body>

</html>